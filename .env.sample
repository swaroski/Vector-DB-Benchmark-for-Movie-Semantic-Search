# Environment variables for Movie Vector Database Benchmark

# Pinecone Configuration (if using Pinecone)
PINECONE_API_KEY=your_pinecone_api_key_here

# OpenAI Configuration (if using OpenAI embeddings)
OPENAI_API_KEY=your_openai_api_key_here

# Weaviate Configuration (if using cloud Weaviate)
WEAVIATE_URL=https://your-cluster.weaviate.network
WEAVIATE_API_KEY=your_weaviate_api_key_here

# TopK Configuration (if using TopK)
TOPK_API_KEY=your_topk_api_key_here
TOPK_REGION=aws-us-east-1-elastica